{
  "name": "jellax",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "library",
    "typescript"
  ],
  "main": "dist/jellax.umd.js",
  "module": "dist/jellax.es5.js",
  "typings": "dist/types/jellax.d.ts",
  "files": [
    "dist"
  ],
  "author": "Robin BAILLARGEAUX <baillargeaux.robin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eldynn/jellax.git"
  },
  "homepage": "https://github.com/Eldynn/jellax#readme",
  "bugs": {
    "url": "https://github.com/Eldynn/jellax/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "npm test -- --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/jest": "^25.1.0",
    "@types/node": "^13.7.5",
    "colors": "^1.3.3",
    "commitizen": "^4.0.0",
    "coveralls": "^3.0.3",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^10.0.8",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.17.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^5.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.0",
    "semantic-release": "^15.13.12",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/node-fetch": "^2.3.4",
    "@types/url-template": "^2.0.28",
    "node-fetch": "^2.6.0",
    "url-template": "^2.0.8"
  }
}
